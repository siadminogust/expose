    "keywords":["ids","intrusion","detection"],
    "homepage":"https://github.com/siadminogust/expose.git",
    "license":"MIT",
    "authors":[
        {
            "name":"Chris Cornutt",
            "email":"ccornutt@phpdeveloper.org",
            "homepage":"http://www.phpdeveloper.org/"
        },
        {
            "name":"Pierre Vassoilles",
            "email":"pierre.vassoilles@gmail.com",
            "homepage":"https://github.com/pierre-vassoilles"
        }
    ],
    "require":{
        "php":">=8.0",
        "twig/twig": "1.*|2.*|3.*",
        "monolog/monolog": "3.*",
        "symfony/console": "6.*"
